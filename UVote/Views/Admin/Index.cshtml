
@{
    ViewBag.Title = "Index";
}
@section Logged{
    @if (Session["employeeId"] == null)
    {
        Response.Redirect(Url.Action("Login", "admin"));
    }
}
<h2>Dashboard</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <ul class="nav nav-pills nav-stacked">
            <li role="presentation">@Html.ActionLink("Add campaign", "Create", "Campaign")</li>
            <li role="presentation">@Html.ActionLink("Add candidate", "Create", "Candidate")</li>
            <li role="presentation">@Html.ActionLink("Add student", "Create", "Student")</li>
        </ul>
    </div>
    <div class="col-md-4">

        <a href="" class="thumbnail">
            <img src="~/Content/Images/active_campaigns.png" alt="Active campaigns">
        </a>

    </div>
</div>